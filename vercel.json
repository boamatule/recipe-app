{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://www.themealdb.com/api/json/v1/"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

